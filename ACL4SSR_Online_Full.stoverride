name: "Custom Rules"
desc: "自定义规则集合"
author: "Assistant"
homepage: "https://github.com/your-repository"
icon: "https://raw.githubusercontent.com/your-repository/icon.png"

override:
  proxy-groups:
    - name: 🚀 节点选择
      type: select
      proxies:
        - 🇭🇰 香港节点
        - 🇨🇳 台湾节点
        - 🇸🇬 狮城节点
        - 🇯🇵 日本节点
        - 🇺🇲 美国节点
        - 🇰🇷 韩国节点
        - 🚀 手动切换
        - DIRECT

    - name: 🚀 手动切换
      type: select
      use:
        - All

    - name: ♻️ 自动选择
      type: url-test
      use:
        - All
      url: http://www.gstatic.com/generate_204
      interval: 300
      tolerance: 50

    - name: 🏷️ 低倍率节点
      type: url-test
      use:
        - All
      filter: '(?i)(0\.[0-9][1-9]|0\.9[0-9]|0\.0[1-9]|0\.[0-9][1-9]x|x0\.[0-9][1-9]|0\.[0-9]x|x0\.[0-9]|0\.[1-9]|0\.[1-9]x|x0\.[1-9])'
      url: http://www.gstatic.com/generate_204
      interval: 300
      tolerance: 50

    - name: ⚡ 高倍率节点
      type: url-test
      use:
        - All
      filter: '^(?!.*(?i)(0\.[0-9][1-9]|0\.9[0-9]|0\.0[1-9]|0\.[0-9][1-9]x|x0\.[0-9][1-9]|0\.[0-9]x|x0\.[0-9]|0\.[1-9]|0\.[1-9]x|x0\.[1-9])).*$'
      url: http://www.gstatic.com/generate_204
      interval: 300
      tolerance: 50

    - name: 💙 其他地区节点
      type: select
      use:
        - All
      filter: '^(?!.*(HK|TW|SG|US|JP|KR|香港|台湾|新加坡|美国|日本|韩国)).*$'

    - name: 📲 电报消息
      type: select
      proxies:
        - 🚀 节点选择
        - ♻️ 自动选择
        - 🏷️ 低倍率节点
        - ⚡ 高倍率节点
        - 🇸🇬 狮城节点
        - 🇭🇰 香港节点
        - 🇨🇳 台湾节点
        - 🇯🇵 日本节点
        - 🇺🇲 美国节点
        - 🇰🇷 韩国节点
        - 🚀 手动切换
        - DIRECT

    - name: 💬 Ai
      type: select
      proxies:
        - 🚀 节点选择
        - 🇸🇬 狮城节点
        - 🇭🇰 香港节点
        - 🇨🇳 台湾节点
        - 🇯🇵 日本节点
        - 🇺🇲 美国节点
        - 🇰🇷 韩国节点
